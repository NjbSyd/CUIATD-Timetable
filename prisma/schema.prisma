generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Timetable {
  id         Int     @id @default(autoincrement())
  class_name String
  day        String
  time_slot  String
  subject    String
  class_room String
  teacher    String
  extra      String?

  @@unique([class_name, day, time_slot])
}